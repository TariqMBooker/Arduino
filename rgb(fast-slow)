int red=8
int green=4
int blue=2
int fast=50
int slow=500
int slower=2000

void setup() {
pinMode(red,OUTPUT);
pinMode(green,OUTPUT);
pinMode(blue,OUTPUT);
}

void loop() {
digitalWrite(red,HIGH);
delay(fast);
digitalWrite(red,LOW);
delay(fast);

digitalWrite(red,HIGH);
delay(fast);
digitalWrite(red,LOW);
delay(fast);

digitalWrite(red,HIGH);
delay(fast);
digitalWrite(red,LOW);
delay(fast);

digitalWrite(red,HIGH);
delay(fast);
digitalWrite(red,LOW);
delay(fast);

digitalWrite(red,HIGH);
delay(fast);
digitalWrite(red,LOW);
delay(fast);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(green,HIGH);
delay(slow);
digitalWrite(green,LOW);
delay(slow);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);

digitalWrite(blue,HIGH);
delay(slower);
digitalWrite(blue,LOW);
delay(slower);
}
